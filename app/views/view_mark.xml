<Alloy>
	<View class="container">
		<View class="viewTopMark">
			<Label class="iconMarkFontSmall" id="iconSoryin" />
		</View>


		<TableView id="table1" class="tableStyle">

			<TableViewRow class="rowStyle">
				<View class="rowTable">
					<View class="leftView">
						<View class="viewHor">
							<Label id="iconUser" class="iconMarkFont"></Label>
							<Label class="labMarkTxt">认证</Label>
						</View>
					</View>
					<View class="rightView">
						<View class="viewHor">
							<Label id="iconLogout" class="iconMarkFont">
							</Label>
							<Label class="labMarkTxt">登出</Label>
						</View>
					</View>
				</View>

			</TableViewRow>
			<TableViewRow class="rowStyle">
				<View class="rowTable">
					<View class="leftView">
						<View class="viewHor">
							<Label id="iconEnter" class="iconMarkFont"></Label>
							<Label class="labMarkTxt">深入</Label>
						</View>
					</View>
					<View class="rightView">
						<View class="viewHor">
							<Label id="iconExpend" class="iconMarkFont"></Label>
							<Label class="labMarkTxt">展开</Label>
						</View>
					</View>

				</View>
			</TableViewRow>

			<TableViewRow class="rowStyle">

				<View class="rowTable">
					<View class="leftView">
						<View class="viewHor">
							<Label id="iconFun" class="iconMarkFont"></Label>
							<Label class="labMarkTxt">功能</Label>
						</View>
					</View>
					<View class="rightView">
						<View class="viewHor">
							<Label id="iconSort" class="iconMarkFont"></Label>
							<Label class="labMarkTxt">筛选</Label>
						</View>
					</View>
				</View>
			</TableViewRow>

			<TableViewRow class="rowStyle">

				<View class="rowTable">
					<View class="leftView">
						<View class="viewHor">
							<Label id="iconSave" class="iconMarkFont"></Label>
							<Label class="labMarkTxt">保存</Label>
						</View>
					</View>
					<View class="rightView">
						<View class="viewHor">
							<Label id="iconFinish" class="iconMarkFont"></Label>
							<Label class="labMarkTxt">完成</Label>
						</View>
					</View>
				</View>
			</TableViewRow>

			<TableViewRow class="rowStyle">
				<View class="rowTable">
					<View class="leftView">
						<View class="viewHor">
							<Label id="iconRet" class="iconMarkFont"></Label>
							<Label class="labMarkTxt">返回</Label>
						</View>
					</View>
				</View>
			</TableViewRow>
		</TableView>


		<View class="viewTopMark">
			<Label class="iconMarkFontSmall" id="iconWorld" />
		</View>

		<TableView id="table2" class="tableStyle">
			<TableViewRow class="rowStyle">
				<View class="rowTable">
					<View class="leftView">
						<View class="viewHor">
							<Label id="iconLogo" class="iconMarkFont"></Label>
							<Label class="labSoryin">缩影Logo</Label>
						</View>
					</View>
				</View>
			</TableViewRow>
		</TableView>
	</View>
</Alloy>